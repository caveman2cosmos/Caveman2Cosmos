<?xml version='1.0' encoding='UTF-8'?>
<Schema xmlns="urn:schemas-microsoft-com:xml-data" xmlns:dt="urn:schemas-microsoft-com:datatypes">
	<!-- base types-->
	<ElementType name="Name" content="textOnly"/>
	<ElementType name="Key" content="textOnly"/>
	<ElementType name="Value" content="textOnly"/>
	<!-- Fader -->
	<ElementType name="Fader">
		<element type="Name"/>
		<element type="Key" minOccurs="0" maxOccurs="*"/>
		<element type="Value" minOccurs="0" maxOccurs="1"/>
	</ElementType>
	<!-- DetailManagerInfos -->
	<ElementType name="DetailManagerInfos" content="eltOnly">
		<element type="Fader" maxOccurs="*"/>
	</ElementType>
</Schema>